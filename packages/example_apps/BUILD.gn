import("//build/ten_runtime/options.gni")

group("example_apps") {
  deps = []

  if (ten_enable_example_packages) {
    if (ten_enable_go_binding) {
      deps += [ "pprof_app_go" ]
    }

    if (ten_enable_python_binding) {
      deps += [ "websocket_asr_local_app" ]
    }
  }
}